service check_mk
{
	type           = UNLISTED
	port           = <%= $::check_mk::agent::config::port %>
	socket_type    = stream
	protocol       = tcp
	wait           = no
	user           = <%= $::check_mk::agent::config::user %>
	server         = <%= $::check_mk::agent::config::server %>
<% if $::check_mk::agent::config::only_from { -%>
	only_from      = 127.0.0.1 <%= $::check_mk::agent::config::only_from %>
<% } -%>
	log_on_success =
	disable        = no
}

